                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="0, 0, 0, 10">
                        <CheckBox Content="Lev. antal" Width="70"  />
                        <TextBox  Height="10" Width="160"/>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="0, 0, 0, 10">
                        <CheckBox Content="Se vedlagte" Width="70"  />
                        <TextBox  Height="10" Width="160"/>
