            <TextBox HorizontalAlignment="Left" Margin="130,340,0,0" TextWrapping="Wrap" Text="Beskrivelse" VerticalAlignment="Top" Width="225" MinHeight="135" Height="Auto"/>
            <TextBlock HorizontalAlignment="Left" Margin="20,345,0,0" TextWrapping="Wrap" Text="Beskrivelse:" VerticalAlignment="Top" FontWeight="Bold"/>
        </Grid>


    </Grid>



</Page>
