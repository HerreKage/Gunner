                    <Button Content="Til forside" Margin="640,50,0,0" VerticalAlignment="Top" Width="125" Foreground="White" Background="Black">
                        <Interactivity:Interaction.Behaviors>
                            <Core:EventTriggerBehavior EventName="Click">
                                <Core:NavigateToPageAction TargetPage="Gunner_OrderList.MainPage"/>
                            </Core:EventTriggerBehavior>
                        </Interactivity:Interaction.Behaviors>
                    </Button>
                </Grid>



                <StackPanel Grid.Column="0" Grid.Row="1">
                    <StackPanel Orientation="Horizontal" Margin="0, 0, 0, 10">
                        <CheckBox Content="Mål" Width="70"  />
                        <TextBox  Height="10" Width="160"/>
                    </StackPanel>


                    <StackPanel Orientation="Horizontal" Margin="0, 0, 0, 10">
                        <CheckBox Content="SUV"  Width="29" />
                            <CheckBox Content="Medie"  Width="29" />
                        </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="0, 0, 0, 10">
                        <CheckBox Content="Sol." Width="71" />
                            <TextBox  Height="10" Width="160"/>
                        </StackPanel>



                    <StackPanel Orientation="Horizontal" Margin="0, 0, 0, 10">
                        <CheckBox Content="SUV"  Width="29" />
                            <CheckBox Content="Medie"  Width="29" />
                        </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="0, 0, 0, 10">
                        <CheckBox Content="Sol." Width="71" />
